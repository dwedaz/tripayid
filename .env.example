# Tripay PPOB Test Environment Variables
# Copy this file to .env and replace with your actual credentials

# Tripay API Key (from your Tripay dashboard)
TRIPAY_API_KEY=your_tripay_api_key_here

# Tripay Secret PIN (from your Tripay dashboard)
TRIPAY_SECRET_PIN=your_secret_pin_here

# Test Mode: sandbox or production
TRIPAY_MODE=sandbox